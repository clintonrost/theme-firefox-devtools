	"tokenColors": [
		{
			"settings": {
				"foreground": "#b1b1b3",
				"invisibles": "#204e8a",
				"lineHighlight": "#204e8a",
				"selection": ""
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"comment": "--theme-highlight-green",
				"foreground": "#86DE74"
			}
		},
		{
			"name": "String",
			"scope": [ "string", "string.quoted" ],
			"settings": {
				"foreground": "#BB9CF1"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ffcd22"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#75bfff"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#a082bd"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#75bfff"
			}
		},
		{
			"name": "Operator",
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "",
				"foreground": "#e0e2e4"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#75bfff"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Function name (definition)",
			"scope": "meta.definition.method",
			"settings": {
				"foreground": "#e0e2e4"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#678cb1"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"comment": "--tag--blue",
				"foreground": "#75bfff"
				// "foreground": "#86CBFA"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"comment": "--tag--pink",
				"foreground": "#FF97E9"
				// "foreground": "#FF95EB"
			}
		},
		{
			"comment": "================ HTML/XML ================"
		},
